{
  "name": "node-gd",
  "version": "1.6.0",
  "description": "GD graphics library (libgd) C++ bindings for Node.js",
  "main": "js/node-gd.js",
  "directories": {
    "test": "test"
  },
  "os": [
    "!win32"
  ],
  "homepage": "https://github.com/y-a-v-a/node-gd",
  "bugs": "https://github.com/y-a-v-a/node-gd/issues",
  "scripts": {
